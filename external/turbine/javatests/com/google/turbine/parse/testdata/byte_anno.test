public class Test {
  @RuntimeAnno
  @Anno(
    b = 42,
    c = 'z',
    s = 43,
    d = 44,
    f = 45,
    i = 46,
    j = 47,
    str = "48",
    z = true
  )
  int a;

  @Anno
  int b;
}