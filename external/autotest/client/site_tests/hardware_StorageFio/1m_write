;
; 1m random writes over a 1 GiB area.
;

[1m_write]
filename=${FILENAME}
size=${FILESIZE}

readwrite=randwrite
bs=1m

ioengine=libaio
iodepth=4
direct=1

do_verify=1
verify=meta
verify_interval=64k
continue_on_error=verify
