;
; 1MB random with verify and mix read and mostly write.
;
[1m_stress]
filename=${FILENAME}
size=${FILESIZE}

readwrite=randrw
rwmixread=15
bs=1m

ioengine=libaio
iodepth=32
direct=1

bwavgtime=5000
iopsavgtime=5000

do_verify=1
verify=meta
verify_interval=64k
verify_dump=1
continue_on_error=verify

; write partition/file twice
loops=2
