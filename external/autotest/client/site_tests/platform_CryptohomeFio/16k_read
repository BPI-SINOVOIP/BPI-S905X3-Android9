;
; 16k random reads over a 1 GiB area.
;

[16k_read]
filename=${FILENAME}
size=${FILESIZE}

time_based
runtime=${RUN_TIME}

ioengine=libaio
iodepth=4

readwrite=randread

bs=16k
norandommap

