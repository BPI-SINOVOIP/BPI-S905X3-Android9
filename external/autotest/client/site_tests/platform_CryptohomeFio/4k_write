;
; 4k random writes over a 1 GiB area.
;

[4k_write]
filename=${FILENAME}
size=${FILESIZE}

time_based
runtime=${RUN_TIME}

ioengine=libaio
iodepth=4

readwrite=randwrite

bs=4k
norandommap

