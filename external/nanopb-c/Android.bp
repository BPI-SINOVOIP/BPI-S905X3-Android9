cc_defaults {
    name: "libprotobuf-c-nano-defaults",
    cflags: [
        "-Wall",
        "-Werror",
        "-Wno-sign-compare",
        "-Wno-unused-parameter",
    ],
    srcs: [
        "pb_decode.c",
        "pb_encode.c",
    ],
    sdk_version: "19",
    export_include_dirs: ["."],
}

cc_library_static {
    name: "libprotobuf-c-nano",
    vendor_available: true,
    defaults: ["libprotobuf-c-nano-defaults"],
}

cc_library_static {
    name: "libprotobuf-c-nano-enable_malloc",
    vendor_available: true,
    defaults: ["libprotobuf-c-nano-defaults"],

    cflags: ["-DPB_ENABLE_MALLOC"],
}

