%mask {s}	1
%mask {w}	1
%mask {w1}	1
%mask {W1}	1
%mask {W2}	1
dnl floating point reg suffix
%mask {D}	1
%mask {imm8}	8
%mask {imms8}	8
%mask {imm16}	16
%mask {reg}	3
%mask {oreg}	3
%mask {reg16}	3
%mask {reg64}	3
%mask {tttn}	4
%mask {mod}	2
%mask {moda}	2
%mask {MOD}	2
%mask {r_m}	3
dnl like {r_m} but referencing byte register
%mask {8r_m}	3
dnl like {r_m} but referencing 16-bit register
%mask {16r_m}	3
dnl like {r_m} but referencing 32- or 64-bit register
%mask {64r_m}	3
%mask {disp8}	8
dnl imm really is 8/16/32 bit depending on the situation.
%mask {imm}	8
%mask {imm64}	8
%mask {imms}	8
