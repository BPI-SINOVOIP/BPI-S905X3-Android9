[Unit]
Description=ChromeOS audio server
PartOf=system-services.target
After=system-services.target

[Service]
Restart=on-failure
LimitRTPRIO=12
TimeoutStopSec=20
ExecStart=/bin/sh /usr/share/cros/init/cras.sh
