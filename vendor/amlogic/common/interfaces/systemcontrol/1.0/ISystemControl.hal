  whiteBalanceGrayPatternSet(int32_t value)
      generates (int32_t ret);

  whiteBalanceGrayPatternGet()
      generates (int32_t ret);

  factorySetHdrMode(int32_t mode)
      generates (int32_t ret);

  factoryGetHdrMode()
      generates (int32_t ret);

  setDnlpParams(int32_t inputSrc, int32_t sigFmt, int32_t transFmt, int32_t level)
