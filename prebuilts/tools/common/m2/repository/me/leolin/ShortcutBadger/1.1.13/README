mvn org.apache.maven.plugins:maven-install-plugin:2.5.1:install-file \
        -Dfile=$OUT/ShortcutBadger-1.1.13.jar \
        -DgroupId=me.leolin \
        -DartifactId=ShortcutBadger \
        -Dversion=1.1.13 \
        -Dpackaging=jar \
        -DgeneratePom=true \
        -DlocalRepositoryPath=repository \
        -DcreateChecksum=true

